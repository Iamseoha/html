    <script>



        $('.header').on('click', function () {
            if ($(this).hasClass('on')) {
                slideUp();
            } else {
                slideUp();
                $(this).addClass('on').next().slideDown();
            }
            function slideUp() {
                $('.header').removeClass('on').next().slideUp();
            };
        })
