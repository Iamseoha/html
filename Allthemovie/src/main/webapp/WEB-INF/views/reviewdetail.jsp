<script>
    $('.more').on('click', function () {
        if ($(this).hasClass('on')) {
            slideUp();
        } else {
            slideUp();
            $(this).addClass('on').next().slideDown();
        }
        function slideUp() {
            $('.more').removeClass('on').next().slideUp();
        };
    })
</script>
